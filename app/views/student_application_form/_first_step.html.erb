<h1>Sending Institution</h1>

<%= form_for@sap, url: student_application_form_path, :method => :post do |f| %>
  

  <div class="field">
    <%= f.label :inst_sending_name %><br />
    <%= f.text_field :inst_sending_name, autofocus: true, autocomplete: "inst_sending_name" %>
  </div>

<div class="field">
    <%= f.label :school_family_dpt %><br />
    <%= f.text_field :school_family_dpt, autofocus: true, autocomplete: "school_family_dpt" %>
  </div>

  <div class="field">
    <%= f.label :inst_adress %><br />
    <%= f.text_field :inst_adress, autofocus: true, autocomplete: "inst_adress" %>
  </div>
   <div class="field">
    <%= f.label :erasmus_code %><br />
    <%= f.text_field :erasmus_code, autofocus: true, autocomplete: "erasmus_code" %>
  </div>
 	<div class="field">
    <%= f.label :dept_coordinator %><br />
    <%= f.text_field :dept_coordinator, autofocus: true, autocomplete: "dept_coordinator" %>
  </div>
	<div class="field">
    <%= f.label :contact_person %><br />
    <%= f.text_field :contact_person, autofocus: true, autocomplete: "contact_person" %>
  </div>
	<div class="field">
    <%= f.label :inst_telephone %><br />
    <%= f.text_field :inst_telephone, autofocus: true, autocomplete: "inst_telephone" %>
  </div>

  <div class="field">
    <%= f.label :inst_email %><br />
    <%= f.text_field :inst_email, autofocus: true, autocomplete: "inst_telephone" %>
  </div>

<%= f.submit "Save current page" %>

<%end%>