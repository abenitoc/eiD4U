<h1>Welcome, <span class="email"> <%=@nominee.email%></span></h1>
 
<%= form_tag @login_url, :method => :post, :id => "saml-form", :class=> "inline" , :enforce_utf8=> false, :authenticity_token => false do %>
  <% if @post_params != nil %>
    <% @post_params.each do |key, value| -%>
      <%= hidden_field_tag key, value %>
    <%end%>
  <% end %>
<% end %>
<%= button_to "Register with email and password", "#{RELATIVE_URL}/register/"+@nominee.registration_token+"/register_with_email_and_password", :method=> :get, form_class: "inline", :class=> "custom-button primary" %>

<%= render partial: "users/eidas_form", :locals => {} %>